@model List<WebApplicationEMPTY.Models.Car.Order>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "/Views/Shared/_Layout.cshtml";

}
<div class="container">
<table class="table table-bordered">
    <thead class ="thead-dark">
    <tr>
        <th scope="col">#</th>
        <th scope="col">Car</th>
        <th scope="col">OrderDetail</th>
        
    </tr>
    </thead>
    <tbody>
   
      
    @foreach (var i in Model)
    {

        
            <tr>
                <th scope="row">@i.id</th>
                <td>@i.email,
                    @i.phone, @i.name
                </td>
                <td>@foreach (var t in i.orderDetails)
                    {
                        @:Модель: @t.car.modelName, 
                        @:цена: @t.car.price.ToString("C")
                    }</td>

            </tr>
        
    }
       
       
    }

    </tbody>
</table>
<a class="btn btn-danger" asp-controller="Order" asp-action="RemoveOrders">Очистить </a>
</div>